<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
      <h1>Edit User Info</h1>
      <%= simple_form_for @user do |f| %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :address %>
        <%= f.input :photo, as: :attachinary %>
        <%= f.submit"edit", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
